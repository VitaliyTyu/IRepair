@page
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model BD9.Pages.ContactInforms.IndexModel
@*@Styles.Render("~/Content/css")*@
<link rel="stylesheet" href="~/css/StyleSheet.css" type="text/css" />
<h2>Список Данных</h2>
<table>
    <tr><th>Фамилия</th><th>Имя</th><th>Отчетсво</th><th>Телефон</th><th>Серия Паспорта</th><th>Номер Паспорта</th><th>СНИЛС</th><th>Адрес регистраци</th><th>Дата рождения</th></tr>
    @foreach (var user in Model.Informs)
    {
            <tr>
                <td>@user.Surname</td>
                <td>@user.Name</td>
                <td>@user.Lastname</td>
                <td>@user.Phone</td>
                <td>@user.PSeries</td>
                <td>@user.PNumber</td>
                <td>@user.Snils</td>
                <td>@user.Adress</td>
                <td>@user.Date</td>
            </tr>
    }
</table>
